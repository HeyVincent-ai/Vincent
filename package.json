{
  "name": "safeskills",
  "version": "1.0.28",
  "description": "Secure secret management service for AI agents",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/index.ts",
    "dev:frontend": "npm --prefix frontend run dev",
    "dev:all": "concurrently -n api,web -c blue,green \"npm run dev\" \"npm run dev:frontend\"",
    "build": "prisma generate && tsc && npm --prefix frontend install && npm --prefix frontend run build",
    "build:backend": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:polymarket": "vitest run src/e2e/polymarket.e2e.test.ts",
    "test:evm-wallet": "vitest run src/e2e/evmWallet.e2e.test.ts",
    "test:raw-signer": "vitest run src/e2e/rawSigner.e2e.test.ts",
    "test:openclaw": "vitest run src/e2e/openclaw.e2e.test.ts --test-timeout 600000",
    "test:openclaw-api": "vitest run src/e2e/openclaw-api.e2e.test.ts --test-timeout 600000",
    "test:openclaw-billing": "vitest run src/e2e/openclaw-billing.e2e.test.ts --test-timeout 120000",
    "test:openclaw-credits": "vitest run src/e2e/openclaw-credits.e2e.test.ts --test-timeout 120000"
  },
  "keywords": [
    "secrets",
    "ai-agents",
    "wallet",
    "security"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.1.0",
    "@types/ssh2": "^1.15.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "concurrently": "^9.2.1",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.1",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@noble/curves": "^2.0.1",
    "@ovhcloud/node-ovh": "^3.0.0",
    "@polymarket/builder-relayer-client": "^0.0.8",
    "@polymarket/clob-client": "^5.2.1",
    "@prisma/client": "^5.22.0",
    "@sentry/node": "^10.38.0",
    "@solana/web3.js": "^1.98.4",
    "@zerodev/ecdsa-validator": "^5.4.9",
    "@zerodev/sdk": "^5.5.7",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "grammy": "^1.39.3",
    "helmet": "^8.1.0",
    "https-proxy-agent": "^7.0.6",
    "prisma": "^5.22.0",
    "ssh2": "^1.17.0",
    "stripe": "^20.3.0",
    "stytch": "^13.1.0",
    "tweetnacl": "^1.0.3",
    "uuid": "^13.0.0",
    "viem": "^2.45.1",
    "zod": "^4.3.6"
  }
}
