{
  "name": "@openclaw/trade-manager",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "trade-manager": "./dist/cli.js"
  },
  "files": [
    "dist",
