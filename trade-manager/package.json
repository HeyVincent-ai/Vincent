{
  "name": "@lit-protocol/trade-manager",
  "version": "0.3.0",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "trade-manager": "./dist/cli.js"
  },
  "files": [
    "dist",
    "prisma/schema.prisma",
    "prisma/migrations",
    "public"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dashboard:dev\"",
    "dashboard:dev": "vite --config dashboard/vite.config.ts",
    "dashboard:build": "vite build --config dashboard/vite.config.ts",
    "build": "npm run dashboard:build && prisma generate && tsc -p tsconfig.json",
    "postinstall": "prisma generate && (node dist/cli.js setup-service || true)",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "start": "node dist/index.js",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "@types/ws": "^8.18.1",
    "axios": "^1.7.9",
    "express": "^4.21.2",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "prisma": "^6.3.1",
    "ws": "^8.19.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.4",
    "@types/pino": "^7.0.4",
    "@types/pino-http": "^5.8.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "lucide-react": "^0.525.0",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "supertest": "^7.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^7.1.2",
    "vitest": "^3.0.5"
  }
}
